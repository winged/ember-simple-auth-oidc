{
  "name": "ember-simple-auth-oidc",
  "version": "0.4.0",
  "description": "A Ember Simple Auth addon which implements the OpenID Connect Authorization Code Flow.",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/adfinis-sygroup/ember-simple-auth-oidc",
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "precommit": "lint-staged",
    "semantic-release": "semantic-release"
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "ember-ajax": "^5.0.0",
    "ember-auto-import": "^1.0.1",
    "ember-cli-babel": "^7.7.3",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-get-config": "^0.2.4",
    "ember-simple-auth": "^1.6.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@adfinis-sygroup/semantic-release-config": "^2.0.0",
    "@ember/jquery": "^0.6.0",
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.10.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-mirage": "^0.4.7",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.0.1",
    "ember-source": "~3.10.0",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.0.0",
    "eslint-config-prettier": "^6.2.0",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-node": "^9.0.1",
    "eslint-plugin-prettier": "2.6.0",
    "husky": "^2.3.0",
    "lint-staged": "^8.1.7",
    "loader.js": "^4.7.0",
    "prettier": "^1.13.7",
    "qunit-dom": "^0.8.4"
  },
  "engines": {
    "node": "8.* || 10.* || >= 12.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "release": {
    "extends": "@adfinis-sygroup/semantic-release-config"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
